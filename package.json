    "minami": "^1.1.1",
    "mongodb": "^3.3.3",
    "mysql": "*",
    "nock": "11.8.0",
    "proxyquire": "^1.8.0",
    "q": "*",
    "redis": "^1.0.0",
    "request": "^2.88.0",
    "restify": "^8.4.0",
    "rimraf": "^2.6.3",
